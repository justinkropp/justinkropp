{
  "name": "justinkropp",
  "version": "1.0.0",
  "scripts": {
    "dev": "npm-run-all --parallel watch:scss serve",
    "watch:scss": "sass --watch main.scss:main.css",
    "serve": "browser-sync start --server --files '*.html, *.css' --no-notify"
  },
  "devDependencies": {
    "browser-sync": "^3.0.2",
    "npm-run-all": "^4.1.5",
    "sass": "^1.69.5"
  }
}
